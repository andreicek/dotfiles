{
  "workbench.colorTheme": "Solarized Light",
  "editor.fontFamily": "MonoLisa Custom",
  "editor.fontLigatures": true,
  "editor.fontSize": 13,
  "breadcrumbs.enabled": false,
  "notebook.breadcrumbs.showCodeCells": false,
  "workbench.iconTheme": null,
  "customizeUI.activityBarHideSettings": true,
  "customizeUI.titleBar": "inline",
  "customizeUI.stylesheet": {
    ".sidebar .title-actions": "visibility: hidden !important",
    ".sidebar .title-label": "visibility: hidden !important",
    ".editor-actions": "display: none"
  },
  "telemetry.telemetryLevel": "off",
  "workbench.editor.untitled.hint": "hidden",
  "todohighlight.keywords": ["DONE:", "NOTE:"],
  "extensions.ignoreRecommendations": true,
  "window.titleBarStyle": "native",
  "elixirLS.suggestSpecs": false,
  "workbench.activityBar.visible": false,
  "workbench.statusBar.visible": false,
  "editor.minimap.enabled": false,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "workbench.startupEditor": "none",
  "editor.guides.indentation": false,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[elixir]": {
    "editor.defaultFormatter": "animus-coop.vscode-elixir-mix-formatter"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.tabSize": 2,
  "editor.detectIndentation": false
}
