{
  "editor.minimap.enabled": false,
  "breadcrumbs.enabled": false,
  "editor.renderWhitespace": "none",
  "workbench.layoutControl.enabled": false,
  "workbench.colorTheme": "Solarized Light",
  "editor.fontFamily": "MonoLisa Custom",
  "editor.fontSize": 13,
  "editor.fontLigatures": true,
  "workbench.iconTheme": null,
  "elixirLS.suggestSpecs": false,
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "editor.guides.indentation": false,
  "telemetry.telemetryLevel": "off",
  "editor.tabSize": 2,
  "editor.detectIndentation": false,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "files.associations": {
    "*.heex": "html-eex"
  },
  "[html-eex]": {
    "editor.defaultFormatter": "animus-coop.vscode-elixir-mix-formatter"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "typescript.updateImportsOnFileMove.enabled": "always",
  "cSpell.userWords": [
    "Appsignal",
    "Contractbook",
    "Cyclomatic",
    "defenum",
    "Floki",
    "hubspot",
    "ilike",
    "nonnulls",
    "Nordigen",
    "pkey",
    "prosemirror",
    "subquery",
    "tast",
    "trunc",
    "upsert"
  ],
  "editor.stickyScroll.enabled": true,
  "workbench.statusBar.visible": false,
  "workbench.startupEditor": "none",
  "problems.decorations.enabled": false,
  "[elixir]": {
    "editor.defaultFormatter": "animus-coop.vscode-elixir-mix-formatter"
  },
  "workbench.activityBar.visible": false,
  "customizeUI.activityBarHideSettings": true,
  "customizeUI.titleBar": "inline",
  "window.titleBarStyle": "native",
  "customizeUI.stylesheet": {
    ".sidebar .title-label": "display: none !important",
    ".sidebar .title-actions": "display: none !important",
    ".editor-actions li": "display: none !important"
  }
}
